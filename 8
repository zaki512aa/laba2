#include <iostream>
using namespace std;

int main() {
    int a = 1, b = 1;
    cout << "1: a = " << a << "  b = " << b << endl;
    for (int i = 2; i <= 15; i++) {
        int A = 3*b + 2*a;
        int B = 2*a + b;
        a = A;
        b = B;
        cout << i << ": a = " << a << "  b = " << b << endl;
    }
    return 0;
}
