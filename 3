#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "vvedite 3 storoni treugolnika po ocheredi" << endl;
    cin >> a >> b >> c;
    if (a + b > c && a + c > b && b + c > a) {
        if (a* a+ b * b == c * c || a * a + c * c == b * b || b * b + c * c == a * a) {
            cout << "YES" << endl;
        }
        else {
            cout << "NO";
        }
    }
    else {
        cout << "UNDEFINED" << endl;
}
    return 0;
}
